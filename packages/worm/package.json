{
  "name": "@dial/worm",
  "version": "0.1.0",
  "private": true,
  "description": "S3WORM entity definitions and data storage for Dial.WTF",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./entities": {
      "types": "./dist/entities/index.d.ts",
      "import": "./dist/entities/index.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.921.0",
    "@aws-sdk/s3-request-presigner": "^3.921.0",
    "@decoperations/s3worm": "^0.3.3",
    "@dial/types": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.8.6",
    "typescript": "^5.6.0"
  }
}
