# Privy Configuration
# Get your Privy App ID from: https://dashboard.privy.io/
NEXT_PUBLIC_PRIVY_APP_ID=your_privy_app_id_here

# Optional: Additional Privy Configuration
# NEXT_PUBLIC_PRIVY_CLIENT_ID=your_client_id_here

# Solana RPC Configuration (optional, using public RPCs by default)
# NEXT_PUBLIC_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
# NEXT_PUBLIC_SOLANA_DEVNET_RPC_URL=https://api.devnet.solana.com

# API Keys for AI Services (if needed)
# OPENAI_API_KEY=your_openai_key_here
# REPLICATE_API_TOKEN=your_replicate_token_here
# ELEVENLABS_API_KEY=your_elevenlabs_key_here

# Storj S3 Configuration (if needed for file storage)
# STORJ_ACCESS_KEY_ID=your_storj_access_key_id
# STORJ_SECRET_ACCESS_KEY=your_storj_secret_access_key
# STORJ_BUCKET_NAME=your_bucket_name
# STORJ_ENDPOINT=https://gateway.storjshare.io
